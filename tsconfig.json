{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es2017", "dom", "esnext.asynciterable", "esnext"], 
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "*": ["*", "src/*"],
      "@components/*": ["src/components/*"]
    },
    "types": ["node", "@testing-library/jest-dom"]
  }
}
